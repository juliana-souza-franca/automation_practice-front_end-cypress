{
  "dependencies": {
    "cypress": "^13.12.0"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "browser:chrome": "npx cypress run --browser=chrome --headed --spec cypress/e2e --reporter mocha-allure-reporter",
    "report:allure": "npx allure generate allure-results --clean -o allure-report && allure open  allure-report"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.29.0",
    "allure-cypress": "^3.0.0-beta.3",
    "mocha-allure-reporter": "^1.4.0"
  }
}
